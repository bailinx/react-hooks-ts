{
  "parser": "babel-eslint",
  "extends": [
    "react-hooks"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "arrow-parens": "off",
    "comma-dangle": [
      "error",
      "never"
    ],
    "no-confusing-arrow": "off",
    "no-unused-expressions": "off",
    "react/jsx-one-expression-per-line": "off",
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  },
  "plugins": [
    "react"
  ]
}
